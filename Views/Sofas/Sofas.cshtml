<script>
    $("a[rel^='prettyPhoto']").prettyPhoto({ animation_speed: 'normal', theme: 'dark', social_tools: false, allow_resize: true, default_width: 500, default_height: 344 });

    $('.photo1').find('span').css({ opacity: 0, marginTop: -139 });
    $('.photo1 > a').hover(function () {
        $(this).find('span').css({ opacity: 0, marginTop: -139 }).stop().animate({ opacity: 1, marginTop: 0 }, 400);
    }, function () {
        $(this).find('span').stop().animate({ opacity: 0, marginTop: 139 }, 400);
    });
</script>